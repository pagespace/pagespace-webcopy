{
  "name": "pagespace-webcopy",
  "version": "1.0.2",
  "description": "Web copy plugin for Pagespace",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagespace/pagespace-webcopy"
  },
  "main": "index.js",
  "scripts": {
    "start": "node standalone",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Phil Mander",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "serve-favicon": "^2.3.0"
  },
  "keywords" : [ "pagespace-plugin" ],
  "pagespace" : {
     "schema" : [{
        "name" : "html",
        "title" : "Web copy",
        "default" : "<p>New web copy</p>",
        "type": "string"
     }, {
         "name" : "wrapperClass",
         "title" : "CSS Class",
         "default" : "webcopy",
         "type" : "string"
     }]
  }
}
